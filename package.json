{
  "name": "nio-style-guide",
  "description": "A Brunch.io skeleton for prototyping/bootstrapping a nio frontend.",
  "author": "J. Holmes",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nioinnovation/brunchy"
  },
  "scripts": {
    "start": "brunch watch --server",
    "build": "brunch build --production",
    "lint": "eslint app/",
    "test": "mocha",
    "verify": "eslint app/ && mocha",
    "clean": "rm -rf public/",
    "sync": "rsync -avzhe ssh --progress --delete public/ n.io:~/subdomains/design",
    "deploy": "npm run clean && brunch b -p && npm run sync"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2.7.1",
    "autoprefixer": "^6.3.6",
    "brunch-static": "^1.1.1",
    "chai": "^3.5.0",
    "clean-css-brunch": "^2.0.0",
    "css-brunch": "^2.6.1",
    "eslint": "^2.11.1",
    "eslint-brunch": "^3.6.0",
    "html-brunch-static": "^1.2.1",
    "javascript-brunch": "^2.0.0",
    "jsenv-brunch": "^2.0.0",
    "marked-brunch-static": "^1.1.0",
    "mocha": "^2.5.3",
    "moment": "^2.13.0",
    "nio-scss": "nioinnovation/nio-scss",
    "postcss-brunch": "^0.5.0",
    "sass-brunch": "^2.6.3",
    "uglify-js-brunch": "^2.0.0",
    "yaml-front-matter": "^3.4.0"
  }
}
